<?xml version="1.0"?>
<jboss-web>
    <context-root>/app/data</context-root>
    <security-domain>lightblue-saml</security-domain>
    <valve>
        <class-name>
            org.picketlink.identity.federation.bindings.tomcat.sp.ServiceProviderAuthenticator
        </class-name>
        <param>
            <param-name>issuerID</param-name>
            <param-value>lightblue</param-value>
        </param>
    </valve>
</jboss-web>
